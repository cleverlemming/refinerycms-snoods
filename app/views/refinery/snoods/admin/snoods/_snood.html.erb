<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(snood) -%>">
  <span class='last_name'>
	  <%= snood.last_name %>
    


  </span>
  <span class='email'>
    
	<%= snood.email %>

  </span>
  <span class='actions'>
    
	<%= action_icon(:preview, refinery.snoods_admin_snood_path(snood), t('.view_live_html')) %>
	<%#job_application_url(job_application)%>
      <%# action_icon(:preview, refinery.snoods_snood_path(snood), t('.view_live_html')) %>
    
    <%= action_icon(:edit,    refinery.edit_snoods_admin_snood_path(snood), t('.edit') ) %>
    <%= action_icon(:delete,  refinery.snoods_admin_snood_path(snood), t('.delete'),
      { class: "cancel confirm-delete",
        data: {confirm: t('message',  scope: 'refinery.admin.delete', title: snood.first_name)}}  ) %>
  </span>
</li>
